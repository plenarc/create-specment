{
  "name": "create-specment",
  "version": "0.2.2",
  "description": "Interactive CLI tool for creating Docusaurus-based specification documentation projects",
  "type": "module",
  "author": "Plenarc",
  "license": "MIT",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "bin": {
    "create-specment": "./bin/index.js"
  },
  "keywords": [
    "cli",
    "documentation",
    "specification",
    "docusaurus",
    "template",
    "generator",
    "interactive"
  ],
  "homepage": "https://github.com/plenarc/create-specment#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/plenarc/create-specment.git"
  },
  "bugs": {
    "url": "https://github.com/plenarc/create-specment/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "check": "biome check --apply .",
    "format": "biome format . --write",
    "lint": "biome lint . --diagnostic-level=error",
    "lint:fix": "biome lint . --fix",
    "typecheck": "tsc",
    "prepublishOnly": "nr build && nr test"
  },
  "dependencies": {
    "@antfu/ni": "^28.0.0",
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "fs-extra": "^11.3.3",
    "inquirer": "^13.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "bin",
    "templates",
    "README.md",
    "LICENSE"
  ]
}
